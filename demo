<script type="text/javascript">
var a = $("#header4").offset().top;

<!-- background color for sticky --> 
$(document).scroll(function(){
    if($(this).scrollTop() > a)
    {   
       $('#header4').css({"background":"#161616"});
    } else {
       $('#header4').css({"background":"transparent"});
    }
});
</script>

